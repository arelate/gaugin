{{define "downloads-page"}}
    <!doctype html>
    <html lang="en">
    {{template "head" .}}
    <body id="downloads">
    {{template "svg-icons"}}
    <div id="downloads-content">
        {{if hasDownloads .CurrentOS}}
            {{template "downloads" .CurrentOS}}
        {{end}}
        {{if hasDownloads .OtherOS}}
            {{template "downloads" .OtherOS}}
        {{end}}
        {{if hasDownloads .Extras}}
            {{template "downloads" .Extras}}
        {{end}}
    </div>
    </body>
    </html>
{{end}}