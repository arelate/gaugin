{{define "changelog-page"}}
    <!doctype html>
    <html lang="en">
    {{template "head" .}}
    <body>
    <div class="changelog-content">
        {{if .Changelog}}
            {{.Changelog}}
        {{else}}
            Changelog is not available for this product
        {{end}}
    </div>
    </body>
    </html>
{{end}}